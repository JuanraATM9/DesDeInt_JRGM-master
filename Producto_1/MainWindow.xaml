<Window x:Class="Adaptacion_PrimerProy_SegundoTrim.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Adaptacion_PrimerProy_SegundoTrim"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <ComboBox x:Name="categoria" SelectionChanged="categoria_SelectionChanged"/>
        <ComboBox x:Name="marca" Margin="100,50,400,100" SelectionChanged="marca_SelectionChanged"/>
        <Button x:Name="boton" Height="20" Width="50" Content="Dale" Click="boton_Click"/>
    </StackPanel>
</Window>
